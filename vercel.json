{
  "version": 2,
  "functions": {
    "api/**/*": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/twilio-token",
      "dest": "/api/twilio-token.js",
      "methods": ["POST"]
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}